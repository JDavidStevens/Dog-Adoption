target.value\r\n    });\r\n  };\r\n  getBreeds() {\r\n    if (this.state.animal) {\r\n      petfinder.breed.list({ animal: this.state.animal }).then(data => {\r\n        if (\r\n          data.petfinder &&\r\n          data.petfinder.breeds &&\r\n          Array.isArray(data.petfinder.breeds.breed)\r\n        ) {\r\n          this.setState({\r\n            breeds: data.petfinder.breeds.breed\r\n          });\r\n        }\r\n      });\r\n    } else {\r\n      this.setState({ breeds: [] });\r\n    }\r\n  }\r\n  render() {\r\n    return (\r\n      <div>\r\n        <NavBar />\r\n        <Provider value={this.state}>\r\n          <Router>\r\n            <LoadableResults path=\"/\" />\r\n            <LoadableDetails path=\"/details/:id\" />\r\n            <LoadableSearchParams path=\"/search-params\" />\r\n          </Router>\r\n        </Provider>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default App;\r\n\r\n//npm run dev runs project in browser on http://localhost:1234\r\n{\r\n  /* \r\n          dump state into DOM:\r\n          <pre>\r\n          <code>{JSON.stringify(this.state, null, 4)}</code>\r\n          </pre> */\r\n}\r\n"},"lineCount":null}},"hash":"66fd7faf2d3cb575b616a485a7f182d2","cacheData":{"env":{"REACT_APP_API_KEY":"e83a921f25913b55e6476026f418f6c0","REACT_APP_API_SECRET":"57963c87ebd39234abf06a800e304a82"}}}